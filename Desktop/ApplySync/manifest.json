{
   "manifest_version": 3,
   "name": "SpeedyApply: Job Application Autofill Tool",
   "short_name": "SpeedyApply",
   "version": "2.13.1",
   "description": "Applying to jobs just got easier. Automate job applications with a single click and track your progress over time.",
   "author": "kirk@speedyapply.com",
   "homepage_url": "https://speedyapply.com",
   "action": {
      "default_popup": "popup.html",
      "default_title": "SpeedyApply: Job Application Autofill Tool"
   },
   "background": {
      "service_worker": "background.js"
   },
   "permissions": ["storage", "tabs", "contextMenus", "sidePanel"],
   "host_permissions": ["*://*/*"],
   "content_scripts": [
      {
         "matches": ["*://*/*"],
         "js": ["content-scripts/content.js"],
         "css": ["content-scripts/content.css"],
         "run_at": "document_idle",
         "all_frames": true
      }
   ],
   "web_accessible_resources": [
      {
         "matches": ["*://*/*"],
         "resources": ["content-scripts/content.css"]
      }
   ],
   "icons": {
      "128": "icon/128.png",
      "16": "icon/16.png",
      "32": "icon/32.png",
      "48": "icon/48.png",
      "96": "icon/96.png"
   },
   "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
   "side_panel": {
      "default_path": "sidepanel.html"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "key": ""
}
